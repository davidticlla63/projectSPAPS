	<parameter name="ID_CONTRAREFERENCIA" class="java.lang.Integer"/>
	<parameter name="LOGO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
	p.nombre AS nombre_medico,
	p.apellido_paterno AS ap_medico,
	p.apellido_materno AS am_medico,
	e.nombre AS especialidad_receptor,
	pa.nombre,
	pa.apellido_paterno,
	pa.apellido_materno,
	pa.edad,
	pa.sexo,
	hc.codigo AS codigo_historia_clinica,
	tr.conducta,
	tr.consulta_externa,
	tr.consulta_guardia,
	tr.impresion_diagnostica,
	tr.sala,
	tr.fecha_modificacion,
	tr.medicoemisor,
	tr.especialidadmedicoemisor,
	tr.usuario_registro,
	sc.descripcion
FROM contrareferencia tr,
	paciente pa,
	historia_clinica hc,
	sucursal sc,
	personal p,
	especialidad e
WHERE 
	 tr.id_historia_clinica = hc.id 
	 AND tr.id_medico_receptor = p.id
	 AND p.id_especialidad = e.id
	 AND hc.id_paciente = pa.id 
	 AND tr.id_sucursal = sc.id 
	 AND tr.id = $P{ID_CONTRAREFERENCIA}]]>
	</queryString>
