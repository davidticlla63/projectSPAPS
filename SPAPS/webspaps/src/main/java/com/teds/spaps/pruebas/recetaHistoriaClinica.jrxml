		<queryString>
			<![CDATA[SELECT 
	rec.administracion,
	m.nombre_generico,
	m.presentacion
FROM detalle_medicamento rec,
	medicamento m,
	personal med,
	paciente pa,
	historia_clinica hc,
	compania co
WHERE 
	 rec.id_compania = hc.id 
	 AND pa.id = hc.id_paciente 
	 AND rec.id_compania = co.id 
	 AND rec.id_medicamento = m.id]]>
		</queryString>
		<field name="nombre_generico" class="java.lang.String"/>
